    "@id": "urn:jbr:experiment-cache-cardinality-small:hookSparqlEndpoint",
    "@type": "HookSparqlEndpointComunica",
    "dockerfileClient": "input/dockerfiles/Dockerfile-client-caching",
    "resourceConstraints": {
      "@type": "StaticDockerResourceConstraints",
      "cpu_percentage": 100
    },
    "configClient": "input/configs-client/config-client-caching-%FACTOR-size%.json",
    "contextClient": "input/context-client.json",
    "additionalBinds": [],
    "clientPort": 3001,
    "clientLogLevel": "warn",
    "queryTimeout": 300,
    "maxMemory": 16384
  }
}