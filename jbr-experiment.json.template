  "queryRunnerReplication": 1,
  "queryRunnerWarmupRounds": 0,
  "queryRunnerRequestDelay": 100,
  "queryRunnerEndpointAvailabilityCheckTimeout": 1000,
  "queryRunnerUrlParams": { "context": "{ \"lenient\": true }" },
